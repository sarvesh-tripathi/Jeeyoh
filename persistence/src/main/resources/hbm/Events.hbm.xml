<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.jeeyoh.persistence.domain.Events" table="events" catalog="jeeyoh">
        <id name="eventId" type="java.lang.Integer">
            <column name="eventId" />
            <generator class="identity" />
        </id>
        <property name="description" type="string">
            <column name="description" />
        </property>
        <property name="event_date" type="timestamp">
            <column name="event_date"/>
        </property>
        <property name="event_date_local" type="timestamp">
            <column name="event_date_local"/>
        </property>
        <property name="event_time_local" type="string">
            <column name="event_time_local" />
        </property>
        <property name="geography_parent" type="string">
            <column name="geography_parent"/>
        </property>
        <property name="venue_name" type="string">
            <column name="venue_name"/>
        </property>
        <property name="city" type="string">
            <column name="city"/>
        </property>
        <property name="state" type="string">
            <column name="state"/>
        </property>
        <property name="genreUrlPath" type="string">
            <column name="genreUrlPath"/>
        </property>
        <property name="urlpath" type="string">
            <column name="urlpath" />
        </property>
        <property name="leaf" type="string">
            <column name="leaf"/>
        </property>
        <property name="channel" type="string">
            <column name="channel"/>
        </property>
        <property name="totalTickets" type="string">
            <column name="totalTickets"/>
        </property>
        <property name="zip" type="string">
            <column name="zip" />
        </property>
        <property name="latitude" type="string">
            <column name="latitude" />
        </property>
        <property name="longitude" type="string">
            <column name="longitude" />
        </property>
        <property name="timezone" type="string">
            <column name="timezone"  />
        </property>
        <property name="timezone_id" type="java.lang.Long">
            <column name="timezone_id" precision="10" scale="0" />
        </property>
        <property name="venue_config_name" type="string">
            <column name="venue_config_name" />
        </property>
        <property name="currency_code" type="string">
            <column name="currency_code" />
        </property>
        <property name="event_config_template_id" type="string">
            <column name="event_config_template_id"  />
        </property>
        <property name="event_config_template" type="string">
            <column name="event_config_template" />
        </property>
        <property name="keywords_en_US" type="string">
            <column name="keywords_en_US" />
        </property>
        <property name="maxPrice" type="java.lang.Double">
            <column name="maxPrice"  />
        </property>
        <property name="minPrice" type="java.lang.Double">
            <column name="minPrice"  />
        </property>
        <property name="maxSeatsTogether" type="java.lang.Double">
            <column name="maxSeatsTogether" />
        </property>
        <property name="minSeatsTogether" type="java.lang.Double">
            <column name="minSeatsTogether" />
        </property>
        <property name="title" type="string">
            <column name="title" />
        </property>
        <property name="ancestorGenreDescriptions" type="string">
            <column name="ancestorGenreDescriptions" />
        </property>
         <many-to-one name="page" class="com.jeeyoh.persistence.domain.Page" fetch="select">
            <column name="pageId" />
        </many-to-one>
        <set name="eventuserlikes" inverse="true" lazy="false">
            <key>
                <column name="eventId" not-null="true" />
            </key>
            <one-to-many class="com.jeeyoh.persistence.domain.Eventuserlikes" />
        </set>
         <set name="usereventsuggestions" inverse="true" lazy="false">
            <key>
                <column name="eventId" not-null="true" />
            </key>
            <one-to-many class="com.jeeyoh.persistence.domain.Usereventsuggestion" />
        </set>
     </class>
</hibernate-mapping>