<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	 <class name="com.jeeyoh.persistence.domain.WallFeedSharing" table="wallfeedsharing" catalog="jeeyoh">
		 <id name="id" type="java.lang.Integer">
	 		<column name="id" />
	 		<generator class="identity"/>
		 </id>
	 	<property name="itemId" type="java.lang.Integer">
	 		<column name="itemId"/>
	 	</property>
	 	<many-to-one name="user" class="com.jeeyoh.persistence.domain.User" fetch="select" lazy ="false">
	 		<column name="userId" not-null="true"/>
	 	</many-to-one>
	 	<many-to-one name="sharedWithUser" class="com.jeeyoh.persistence.domain.User" fetch="select">
	 		<column name="sharedWithUserId" not-null="true"/>
	 	</many-to-one>
	 	<many-to-one name="funboard" class="com.jeeyoh.persistence.domain.Funboard" fetch="select" lazy ="false">
	 		<column name="funBoardId" not-null="true"/>
	 	</many-to-one>
	 	<property name="category" type="string">
	 		<column name="category"/>
	 	</property>
	 	<property name="createdTime" type="timestamp">
	 		<column name="createdTime"/>
	 	</property>
	 	<property name="updatedTime" type="timestamp">
	 		<column name="updatedTime"/>
	 	</property>
	 	<property name="itemType" type="string">
	 		<column name="itemType"/>
	 	</property>
	 	<property name="itemRank" type="java.lang.Integer">
	 		<column name="itemRank"/>
	 	</property>
	 	<property name="packageName" type="string">
	 		<column name="packageName"/>
	 	</property>
	 	<property name="packageRank" type="java.lang.Double">
	 		<column name="packageRank"/>
	 	</property>
	 </class>
</hibernate-mapping>